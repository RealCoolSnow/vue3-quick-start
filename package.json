{
  "name": "vue3-quick-start",
  "description": "Quick start project with Vue3 & Vite & TypeScript.",
  "version": "1.0.0",
  "private": true,
  "author": "CoolSnow(coolsnow2020@gmail.com)",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --port 3001 --open",
    "build": "cross-env NODE_ENV=production vite build",
    "build-dev": "cross-env NODE_ENV=production vite build --mode devserver",
    "test": "cross-env NODE_ENV=development jest -i",
    "scan": "f2elint scan",
    "fix": "f2elint fix"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.1",
    "axios": "^0.24.0",
    "vue": "^3.2.26",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "autoprefixer": "latest",
    "cross-env": "^7.0.3",
    "f2elint": "^2.1.0",
    "jest": "^27.4.5",
    "mockjs": "^1.1.0",
    "pnpm": "^6.24.4",
    "postcss": "latest",
    "tailwindcss": "^3.0.9",
    "ts-jest": "^27.1.2",
    "typescript": "4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-components": "^0.11.4",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.19.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
